<section class="other">
  <div class="container" data-aos="fade">
    <div class="other-text">
      <h1 data-aos="fade-up" data-aos-delay="300">Style Guide</h1>
      <p class="path" data-aos="fade" data-aos-duration="400">
        <a href="/">Home</a> <i class="fas fa-angle-right"></i> Style Guide
      </p>
      <p class="desc" data-aos="fade-down" data-aos-duration="500">
        Ready to take your startup to the next level? Letâ€™s connect. Whether
        you're looking to build your first product, scale your technology, or
        explore collaboration
      </p>
    </div>
  </div>
</section>

<section class="style-guide">
  <div class="container">

    <!-- ================= COLOR PALETTE ================= -->
    <div class="style-box" data-aos="fade-up">
      <h1 class="style-title">Color Palette</h1>

      <!-- Brand -->
      <div class="style-row" data-aos="fade-up" data-aos-delay="100">
        <div class="style-label">
          <span>Brand Colors</span>
          <div class="broken-line"></div>
        </div>
        <div class="color-cards">
          <div class="color-card brand-1">
            <p>Primary Brand</p><span>#9A4DFF</span>
          </div>
          <div class="color-card brand-2">
            <p>Brand Accent</p><span>#B56BFF</span>
          </div>
          <div class="color-card brand-3">
            <p>Brand Dark</p><span>#6A0DAD</span>
          </div>
        </div>
      </div>

      <!-- Text -->
      <div class="style-row" data-aos="fade-up" data-aos-delay="150">
        <div class="style-label">
          <span>Text Colors</span>
          <div class="broken-line"></div>
        </div>
        <div class="color-cards">
          <div class="color-card text-1">
            <p>Primary Text</p><span>#FFFFFF</span>
          </div>
          <div class="color-card text-2">
            <p>Secondary Text</p><span>#CCCCCC</span>
          </div>
          <div class="color-card text-3">
            <p>Muted Text</p><span>#888888</span>
          </div>
        </div>
      </div>

      <!-- System -->
      <div class="style-row" data-aos="fade-up" data-aos-delay="200">
        <div class="style-label">
          <span>System Colors</span>
          <div class="broken-line"></div>
        </div>
        <div class="color-cards">
          <div class="color-card sys-1">
            <p>Success</p><span>#00FFCC</span>
          </div>
          <div class="color-card sys-2">
            <p>Error</p><span>#FF4D4D</span>
          </div>
          <div class="color-card sys-3">
            <p>Warning</p><span>#FFAA00</span>
          </div>
          <div class="color-card sys-4">
            <p>Info</p><span>#4D79FF</span>
          </div>
        </div>
      </div>

      <!-- Base -->
      <div class="style-row" data-aos="fade-up" data-aos-delay="250">
        <div class="style-label">
          <span>Base Colors</span>
          <div class="broken-line"></div>
        </div>
        <div class="color-cards">
          <div class="color-card base-1">
            <p>Main Background</p><span>#111111</span>
          </div>
          <div class="color-card base-2">
            <p>Surface</p><span>#1A1A1A</span>
          </div>
        </div>
      </div>
    </div>

    <!-- ================= TYPOGRAPHY ================= -->
    <div class="style-box" data-aos="fade-up">
      <h1 class="style-title">Typography</h1>

      <div class="type-block">
        <div class="style-label"><span>Heading 1</span><div class="broken-line"></div></div>
        <h1 class="preview-h1">The five boxing wizards jump quickly.</h1>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Heading 2</span><div class="broken-line"></div></div>
        <h2 class="preview-h2">Pack my box with five dozen liquor jugs.</h2>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Heading 3</span><div class="broken-line"></div></div>
        <h3 class="preview-h3">When zombies arrive, quickly fax Judge Pat.</h3>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Heading 4</span><div class="broken-line"></div></div>
        <h4 class="preview-h4">Amazingly few discotheques provide jukeboxes.</h4>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Heading 5</span><div class="broken-line"></div></div>
        <h5 class="preview-h5">Go, lazy fat vixen; be shrewd, jump quick.</h5>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Heading 6</span><div class="broken-line"></div></div>
        <h6 class="preview-h6">The quick onyx goblin jumps over the lazy dwarf.</h6>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Body</span><div class="broken-line"></div></div>
        <p class="body-text">
          Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt.
        </p>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Accent</span><div class="broken-line"></div></div>
        <p class="accent-text">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
      </div>

      <div class="type-block">
        <div class="style-label"><span>Small Text</span><div class="broken-line"></div></div>
        <p class="small-text">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
      </div>
    </div>

    <!-- ================= COMPONENTS ================= -->
    <div class="style-box" data-aos="fade-up">
      <h1 class="style-title">Components</h1>

      <!-- Buttons -->
      <div class="component-block">
        <div class="style-label"><span>Buttons</span><div class="broken-line"></div></div>
        <div class="button-grid">
          <button class="btn primary xl">Primary XL</button>
          <button class="btn primary lg">Primary LG</button>
          <button class="btn primary md">Primary MD</button>
          <button class="btn primary sm">Primary SM</button>
          <button class="btn primary xs">Primary XS</button>
          <button class="btn secondary md">Secondary</button>
          <button class="btn accent md">Accent</button>
        </div>
      </div>

      <!-- Icon Buttons -->
      <div class="component-block">
        <div class="style-label"><span>Icon Buttons</span><div class="broken-line"></div></div>
        <div class="button-grid">
          <button class="btn primary md">
            Leading Value <i class="fas fa-angle-up"></i>
          </button>
          <button class="btn secondary md">
            Trailing Action <i class="fas fa-angle-down"></i>
          </button>
        </div>
      </div>

      <!-- Counters -->
      <div class="component-block">
        <div class="style-label"><span>Counters</span><div class="broken-line"></div></div>
        <div class="counter-grid">
          <div class="counter">
            <span class="purecounter" data-purecounter-end="72"></span><small>K+</small>
            <p>Daily Transactions</p>
          </div>
          <div class="counter">
            <span class="purecounter" data-purecounter-end="45"></span><small>K+</small>
            <p>Security Guaranteed</p>
          </div>
          <div class="counter">
            <span class="purecounter" data-purecounter-end="99"></span><small>%</small>
            <p>Integrated Merchants</p>
          </div>
          <div class="counter">
            <span class="purecounter" data-purecounter-end="500"></span><small>+</small>
            <p>Global Partners</p>
          </div>
        </div>
      </div>
    </div>

  </div>
</section>


