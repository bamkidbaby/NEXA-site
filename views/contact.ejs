<section class="contact-section">
  <div class="contact-container">

    <!-- LEFT SIDE -->
    <div class="contact-left">
      <h2>Technology That Grows With You</h2>
      <p>
        From early-stage startups to worldwide enterprises, our flexible pricing
        evolves with your needs. Begin free, scale effortlessly, and unlock
        features that accelerate your team's innovation.
      </p>

      <div class="info-cards">
        <div class="info-card">
          <i class="fas fa-envelope"></i>
          <h4>Email</h4>
          <span>hello@yourcompany.com</span>
        </div>

        <div class="info-card">
          <i class="fas fa-phone"></i>
          <h4>Phone</h4>
          <span>+1 (234) 567-8901</span>
        </div>

        <div class="info-card">
          <i class="fas fa-flag"></i>
          <h4>Address</h4>
          <span>123 Innovation Street, New York, NY</span>
        </div>
      </div>
    </div>

    <!-- RIGHT SIDE (FORM) -->
    <div class="contact-right">
      <form class="contact-form">
        <div class="form-row">
          <input type="text" placeholder="First Name" required />
          <input type="text" placeholder="Last Name" required />
        </div>

        <div class="form-row">
          <input type="email" placeholder="Email Address" required />
          <input type="text" placeholder="Subject" required />
        </div>

        <textarea placeholder="Comments" rows="5"></textarea>

        <button type="submit">Send Message</button>
      </form>
    </div>

  </div>
</section>


